{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "mocha": true,
    "mongo": true
  },
  "rules": {
    "padded-blocks": "off",
    "arrow-parens": "off",
    "consistent-return": "warn",
    "no-unused-vars": [
      "error", {
        "vars": "local",
        "args": "none"
      }
    ],
    "comma-dangle": "off"
  }
}
